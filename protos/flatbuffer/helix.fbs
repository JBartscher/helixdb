namespace Helix;

table Entry {
    timestamp: Timestamp ;
    key: [ubyte];
    value: [ubyte];
}

// todo: no need?
struct Timestamp {
    timestamp: long;
}

struct TimeRange {
    start: Timestamp;
    end: Timestamp;
}

struct LevelId {
    id: long;
}

struct LevelDesc {
    time_range: TimeRange;
    id: LevelId;
}

table LevelInfo {
    infos: [LevelDesc];
}
